{
    "name": "ddetyuk/dompdf-module",
    "type": "library",
    "description": "A Laminas module for incorporating Dompdf support.",
    "keywords": ["pdf","dompdf", "zf3", "laminas"],
    "homepage": "http://raymondkolbe.com",
    "license": "MIT",
    "authors": [
        {
        "name": "Raymond J. Kolbe",
        "email": "rkolbe@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "laminas/laminas-mvc": "^3.1",
        "dompdf/dompdf": "^0.8.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8.27",
        "codeclimate/php-test-reporter": "^0.3.2",
        "squizlabs/php_codesniffer": "^2.7",
        "phpmd/phpmd": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "DompdfModule\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DompdfModule\\": "tests/DompdfModule"
        }
    }
}
